{
	"compilerOptions": {
		"target": "es5",
		"module": "es2015",
		"moduleResolution": "node",
		"jsx": "react",
		"jsxFactory": "app.h",
		"jsxFragmentFactory": "app.Fragment",
		"lib": ["dom", "esnext", "es5"],
		"experimentalDecorators": true,
		"esModuleInterop": true,
		"outDir": "../esm",
		"sourceMap": true,
		"skipLibCheck": true
	}
}