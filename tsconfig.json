{
	"compilerOptions": {
		"target": "es2018",
		"module": "es2015",
		"moduleResolution": "node",
		"jsx": "react",
		"jsxFactory": "app.h",
		"jsxFragmentFactory": "app.Fragment",
		"lib": ["dom", "es2018", "esnext.asynciterable"],
		"experimentalDecorators": true,
		"sourceMap": true,
		"esModuleInterop": true,
		"downlevelIteration": true,
		"skipLibCheck": true
	}
}
